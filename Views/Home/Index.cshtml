@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h3>Vehículo</h3>
    <div class="form-floating mb-3">
        <select class="form-select" id="MarcaSelect" aria-label="Floating label select example">
            <option selected value='0'>Seleccione una Marca</option>
        </select>
        <label for="floatingSelect">Marca:</label>
    </div>
    <div class="form-floating mb-3">
        <select class="form-select" id="SubmarcaSelect" aria-label="Floating label select example">
            <option selected value='0'>Seleccione una Submarca</option>
        </select>
        <label for="floatingSelect">Submarca:</label>
    </div>
    <div class="form-floating mb-3">
        <select class="form-select" id="ModeloSelect" aria-label="Floating label select example">
            <option selected value='0'>Seleccione un modelo</option>
        </select>
        <label for="floatingSelect">Modelo:</label>
    </div>
    <div class="form-floating mb-3">
        <select class="form-select" id="DescripcionSelect" aria-label="Floating label select example">
            <option selected value='0'>Seleccione una descripcion</option>
        </select>
        <label for="floatingSelect">Descripcion:</label>
    </div>
    <h3>Domicilio</h3>
    <div class="form-floating mb-3">
        <input type="number" class="form-control" id="PostalCode" placeholder="12345">
        <label for="floatingInput">Codigo Postal</label>
    </div>
    <div class="form-floating mb-3">
        <input type="text" readonly="readonly" class="form-control" id="Estado" placeholder="Estado">
        <label for="floatingSelect">Estado:</label>
    </div>
    <div class="form-floating mb-3">
        <input type="text" readonly="readonly" class="form-control" id="Municipio" placeholder="Municipio">
        <label for="floatingSelect">Municipio:</label>
    </div>
    <div class="form-floating mb-3">
        <select class="form-select" id="Colonia" aria-label="Floating label select example">
            <option selected value='0'>Seleccione una colonia</option>
        </select>
        <label for="floatingSelect">Colonia:</label>
    </div>
    <button type="button" id="SubmitBtn" class="btn btn-primary">Cotizar</button>
</div>
